
class rec
{
    static int a = 1;
    public static void power(int x, int n)
    {
        if(n==0)
            System.out.println(a);
        else{

        a = a * x;
        n--;
        power(x, n);
    }

    }
    public static void main(String[]args)
    {
        int x=12;
        int n=2;
        power(x, n);
    }
}
